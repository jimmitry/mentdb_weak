script create post "perf.thought" false 1 
  (param;) 
  "" 
{

	#Save the start time;
	-> "[start_ms]" (date current_ms);

	for (-> "[i]" 0) (< [i] 100) (++ "[i]") {

		thought insert orange "" fr;

	};

	#Save the end time;
	-> "[end_ms]" (date current_ms);

	exception (1) (- [end_ms] [start_ms]);

} "";
script set delay "perf.thought.post" 0 day {1;};
