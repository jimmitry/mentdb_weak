script create post "perf.node" false 1 
  (param;) 
  "" 
{

	#Save the start time;
	-> "[start_ms]" (date current_ms);

	for (-> "[i]" 0) (< [i] 100) (++ "[i]") {

		node create (concat "node_id_" [i]);

	};

	#Save the end time;
	-> "[end_ms]" (date current_ms);

	exception (1) (- [end_ms] [start_ms]);

} "";
script set delay "perf.node.post" 0 day {1;};
