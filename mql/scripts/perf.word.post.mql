script create post "perf.word" false 1 
  (param;) 
  "" 
{

	#Save the start time;
	-> "[start_ms]" (date current_ms);

	for (-> "[i]" 0) (< [i] 100) (++ "[i]") {

		word create (concat (string generate_random_str 7) [i]) fr;

	};

	#Save the end time;
	-> "[end_ms]" (date current_ms);

	exception (1) (- [end_ms] [start_ms]);

} "";
script set delay "perf.word.post" 0 day {1;};
