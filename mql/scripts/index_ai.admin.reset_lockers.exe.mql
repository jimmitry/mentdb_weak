script create exe "index_ai.admin.reset_lockers" false 1 
  (param
  	(var "[db_id]" {true} "The database id" is_null:false is_empty:false "db1")
  ;) 
  "Reset lockers" 
{

	json load "cmds" "[]";

	json load "cmd" "{}";
	json iobject "cmd" / t "RESET_LOCKERS" "STR";
	json iobject "cmd" / db [db_id] "STR";

	json iarray "cmds" / (json doc "cmd") OBJ;
	
	index_ai execute (json doc "cmds");
	
} "Return nothing";
script set delay "index_ai.admin.reset_lockers.exe" 0 day {1;};
