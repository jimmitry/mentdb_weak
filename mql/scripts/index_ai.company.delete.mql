script create delete "index_ai.company" false 1 
  (param
  	(var "[db_id]" {true} "The database id" is_null:false is_empty:false "db1")
  	(var "[city_id]" {true} "The city id" is_null:false is_empty:false "1")
  	(var "[company]" {true} "The company id" is_null:false is_empty:false "Innov-AI")
  ;) 
  "Delete all relations for a specific company" 
{

	json load "cmds" "[]";

	json load "cmd" "{}";
	json iobject "cmd" / t "DEL_COMPANY" "STR";
	json iobject "cmd" / db [db_id] "STR";
	json iobject "cmd" / cy [city_id] "STR";
	json iobject "cmd" / c [company] "STR";

	json iarray "cmds" / (json doc "cmd") OBJ;
	
	index_ai execute (json doc "cmds");
	
} "Return nothing";
script set delay "index_ai.company.delete" 0 day {1;};
